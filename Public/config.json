{
    "trailing_slashes": true,
    "redirects": [
        {
            "req": "/e",
            "src": "https://sendsecretmsg.web.app",
            "status": 301
        }
    ],
    "rewrites": [
        {
            "req": "/m",
            "src": "/msg/",
            "status": 301
        }
    ],
    "forbidden": [
        "/config.json",
        "/jshpinfo"
    ],
    "errors": {
        "404": "/404.jshp.html",
        "403": "/403.jshp.html"
    }
}
