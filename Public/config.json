{
    "noExtension": [
        ".jshp.html",
        ".html"
    ],
    "hotCompile": false,
    "trailingSlashes": true,
    "redirects": [
        {
            "req": "/github",
            "src": "https://github.com/AvirukBasak/jshp-app",
            "status": 301
        },
        {
            "req": "/test",
            "src": "javascript:alert(document.cookie);",
            "status": 302
        }
    ],
    "rewrites": [
        {
            "req": "/rwt1",
            "src": "/config/"
        },
        {
            "req": "/rwt2",
            "src": "/logs/"
        }
    ],
    "errFiles": {
        "404": "/404.html"
    }
}
